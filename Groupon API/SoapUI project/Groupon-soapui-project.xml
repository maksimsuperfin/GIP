testRunner.testCase.setPropertyValue("division_id", "")

def divisions = context.expand( '${Get All country divisions#Response#$.divisions}' )
def slurper = new JsonSlurper() 

def jsonList = slurper.parseText(divisions) //parse the JSON
//log.info jsonList
assert jsonList instanceof List

List&lt;String> divisionIDs = new ArrayList&lt;String>()
//log.info divisionIDs
jsonList.each{
	divisionIDs.add(it.id)
}
//log.info divisionIDs
